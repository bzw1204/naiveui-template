{
  "name": "naive-ui-template",
  "type": "module",
  "version": "1.1.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "prepare": "npx simple-git-hooks",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit --composite false"
  },
  "dependencies": {
    "@unhead/vue": "^2.0.12",
    "colord": "^2.9.3",
    "mitt": "^3.0.1",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "unplugin-vue-markdown": "^29.1.0",
    "vue-color-kit": "^1.0.6",
    "vue-draggable-plus": "^0.6.0",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.0.0",
    "@biomejs/biome": "2.1.2",
    "@iconify-json/ant-design": "^1.2.5",
    "@iconify-json/carbon": "^1.2.11",
    "@iconify-json/fa": "^1.2.1",
    "@iconify-json/fa-brands": "^1.2.1",
    "@iconify-json/fe": "^1.2.2",
    "@iconify-json/fluent": "^1.2.28",
    "@iconify-json/ion": "^1.2.4",
    "@iconify-json/mingcute": "^1.2.3",
    "@iconify-json/tabler": "^1.2.20",
    "@iconify/utils": "^2.3.0",
    "@skit/x.naive-ui": "^0.19.0",
    "@types/node": "^22.15.17",
    "@unocss/eslint-config": "^66.3.3",
    "@unocss/eslint-plugin": "^66.3.3",
    "@unocss/preset-icons": "^66.3.3",
    "@unocss/preset-rem-to-px": "^66.3.3",
    "@unocss/transformer-directives": "^66.3.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vueuse/core": "^13.5.0",
    "alova": "^3.3.4",
    "eslint": "^9.31.0",
    "eslint-plugin-format": "^1.0.1",
    "less": "^4.4.0",
    "lint-staged": "^16.1.2",
    "naive-ui": "^2.42.0",
    "ogl": "^1.0.11",
    "only-allow": "^1.2.1",
    "pro-naive-ui": "^2.4.4",
    "radash": "^12.1.1",
    "sass": "^1.89.2",
    "simple-git-hooks": "^2.13.0",
    "tinycolor2": "^1.6.0",
    "typescript": "^5.8.3",
    "unocss": "^66.3.3",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.5.0",
    "vite": "npm:rolldown-vite@^6.3.18",
    "vite-plugin-vue-mcp": "^0.3.2",
    "vite-plugin-vue-meta-layouts": "^0.5.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue": "3.5.18",
    "vue-tsc": "^3.0.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "simple-git-hooks",
      "vue-demi"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm typecheck",
    "pre-push": "npx lint-staged"
  },
  "lint-staged": {
    "*": [
      "git add ."
    ],
    "*.{ts,tsx,vue}": [
      "eslint --fix"
    ],
    "*.{css,less}": [
      "prettier --write"
    ]
  }
}
